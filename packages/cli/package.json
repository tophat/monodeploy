{
  "name": "monodeploy",
  "version": "3.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tophat/monodeploy.git",
    "directory": "packages/cli"
  },
  "author": "Top Hat Monocle Corp. <opensource@tophat.com>",
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "bin": "./lib/index.js",
    "main": "./lib/index.js",
    "access": "public",
    "exports": {
      ".": "./lib/index.js",
      "./preset-recommended": "./lib/presets/recommended.js"
    }
  },
  "group": "monodeploy",
  "bin": "./src/index.ts",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./preset-recommended": "./src/presets/recommended.ts"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "run workspace:clean \"$(pwd)\"",
    "types:watch": "run workspace:types:watch \"$(pwd)\"",
    "test:watch": "run workspace:test:watch \"$(pwd)\"",
    "build:watch": "run workspace:build:watch \"$(pwd)\"",
    "prepack": "run workspace:build \"$(pwd)\""
  },
  "devDependencies": {
    "@types/node": "^14.17.14"
  },
  "dependencies": {
    "@monodeploy/logging": "workspace:^3.3.0",
    "@monodeploy/node": "workspace:^3.3.0",
    "@monodeploy/types": "workspace:^3.2.0",
    "@yarnpkg/core": "^3.2.4",
    "@yarnpkg/fslib": "^2.7.1",
    "ajv": "^8.11.0",
    "clipanion": "^3.2.0-rc.11",
    "typanion": "^3.9.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
